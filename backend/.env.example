# Stream (Video/Audio Transport) - Get from https://getstream.io/dashboard/
STREAM_API_KEY=your_stream_api_key
STREAM_API_SECRET=your_stream_api_secret

# Google Gemini (Vision + LLM) - Get from https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key

# ElevenLabs (Text-to-Speech) - Get from https://elevenlabs.io/
ELEVENLABS_API_KEY=your_elevenlabs_api_key
# Optional: Custom voice ID (default is "Chris")
# ELEVENLABS_VOICE_ID=Anr9GtYh2VRXxiPplzxM

# Roboflow (Object Detection) - Optional, for player/ball detection
# Get from https://roboflow.com/
# ROBOFLOW_API_KEY=your_roboflow_api_key

# Google Cloud Storage (Video Storage) - Optional, for saving recordings
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/gcp-key.json
